{
  "name": "@r2eagle/chartflow",
  "version": "1.0.4",
  "description": "Runtime library for ChartFlow - Create beautiful Chart.js charts in Webflow Designer",
  "main": "dist/runtime.min.js",
  "types": "dist/runtime.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "webflow",
    "chart.js",
    "charts",
    "visualization",
    "webflow-extension",
    "data-visualization"
  ],
  "author": "r2eagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r2eagle/chartly.git"
  },
  "homepage": "https://github.com/r2eagle/chartly#readme",
  "bugs": {
    "url": "https://github.com/r2eagle/chartly/issues"
  },
  "scripts": {
    "dev": "npm install && concurrently \"npm run serve\" \"npm run watch-webpack\"",
    "serve": "webflow extension serve",
    "build": "npm run build-webpack && npm run bundle",
    "bundle": "webflow extension bundle",
    "watch-webpack": "webpack --config webpack.config.mjs --mode development --watch",
    "build-webpack": "webpack --config webpack.config.mjs --mode production",
    "build-runtime": "node scripts/build-runtime.js",
    "prepublishOnly": "npm run build-runtime",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@types/node": "^25.0.8",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@webflow/designer-extension-typings": "^2.0.2",
    "@webflow/webflow-cli": "^1.10.0",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.5.7",
    "eslint-plugin-promise": "^6.1.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.19",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "lucide-react": "^0.563.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
